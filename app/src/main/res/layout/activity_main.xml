<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CCC6C6"
    android:layoutDirection="rtl"
    android:textDirection="rtl"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="287dp"
        android:layout_height="40dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="الاسم ثلاثي"
        android:inputType="textPersonName"
        android:textColorHint="#5A5A5A"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="114dp" />

    <EditText
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="287dp"
        android:layout_height="40dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="اسم الجامعة"
        android:inputType="textPersonName"
        android:textColorHint="#5A5A5A"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="284dp"
        android:layout_height="26dp"
        android:text="الحالة الاكاديمية:"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="319dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="287dp"
        android:layout_height="39dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="السجل المدني"
        android:inputType="textPersonName"
        android:textColorHint="#5A5A5A"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="168dp" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="287dp"
        android:layout_height="39dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="التخصص"
        android:inputType="textPersonName"
        android:textColorHint="#5A5A5A"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="274dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="99dp"
        android:layout_height="29dp"
        android:text="انشاء حساب"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="70dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="287dp"
        android:layout_height="39dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="البريد الالكتروني"
        android:inputType="textEmailAddress"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="414dp" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="341dp"
        android:layout_height="40dp"
        android:layoutDirection="rtl"
        android:shadowColor="#000000"
        android:text="أتعهد بأن جميع المعلومات اعلاه صحيحة و أتحمل كامل المسؤولية عنها."
        android:textColor="#FF0000"
        android:textColorLink="#0E0D0D"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="527dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="287dp"
        android:layout_height="40dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="كلمة المرور"
        android:inputType="textPassword"
        android:textAlignment="textEnd"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="473dp" />

    <Button
        android:id="@+id/login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#8C8987"
        android:text="تسجيل"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="577dp" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="53dp"
        android:layout_height="42dp"
        android:background="#CDC9C9"
        android:layoutDirection="ltr"
        android:tint="#000000"
        app:srcCompat="?attr/actionModeCloseDrawable"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="#CDC9C9"
        android:tint="#434242"
        app:srcCompat="@android:drawable/ic_dialog_dialer"
        tools:layout_editor_absoluteX="349dp"
        tools:layout_editor_absoluteY="16dp" />

    <RadioGroup
        android:layout_width="163dp"
        android:layout_height="65dp"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="341dp">

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layoutDirection="rtl"
            android:shadowColor="#0E0E0E"
            android:text="طالب"
            android:textColorLink="#000000" />

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layoutDirection="rtl"
            android:shadowColor="#100F0F"
            android:text="متخرج"
            android:textColorLink="#0B0A0A" />
    </RadioGroup>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="90dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/twaq"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="9dp" />

</androidx.constraintlayout.widget.ConstraintLayout>